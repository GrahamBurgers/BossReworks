    <LuaComponent
        script_source_file="mods/boss_reworks/files/spells/reward_gate.lua"
    ></LuaComponent>

	<VariableStorageComponent
		name="projectile_file"
		value_string="mods/boss_reworks/files/spells/reward_gate.xml"
	></VariableStorageComponent>

</Entity>